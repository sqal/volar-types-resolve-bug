{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": false,
    "resolveJsonModule": true
  }
}
